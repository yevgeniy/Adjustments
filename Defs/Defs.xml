<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastGoToTargetBase">
		<defName>ADJ_SoulLeech</defName>
		<label>soul leech</label>
		<description>Slowly leach the soul from a hapless pawn storing it as a reserve conciousness.</description>
		<iconPath>Abilities/Puppeteer/BrainLeech</iconPath>
		<abilityClass>Adjustments.Puppeteer_Adjustments.Ability_SoulLeech</abilityClass>
		<castSound>VPEP_Brainleech_Cast</castSound>
		<targetMode>Pawn</targetMode>
		<distanceToTarget>1.5</distanceToTarget>
		<needsTicking>true</needsTicking>
		<drawAimPie>false</drawAimPie>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<castTime>50</castTime>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEP_Puppeteer</path>
				<level>1</level>
				<order>2</order>
				<psyfocusCost>0.1</psyfocusCost>
				<entropyGain>0</entropyGain>
				<psychic>True</psychic>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
	<VFECore.Abilities.AbilityDef >
	
		<defName>ADJ_SoulGrowth</defName>
		
		<label>soul growth</label>
		<description>Slowly grow the conciousness of a puppet by using reserve leeched conciousness.  Will grow with diminishing returns and will not go beyond 130%.</description>
		<warmupSound>VPE_PsycastCastingEffect</warmupSound>
        <warmupMote>VPE_Mote_CastPsycast</warmupMote>
		<gizmoClass>VanillaPsycastsExpanded.Command_Ability_Psycast</gizmoClass>
		<cooldownTime>120</cooldownTime>
		<rangeRingColor>(1, 1, 1)</rangeRingColor>
		<radiusRingColor>(0.3, 0.8, 1)</radiusRingColor>
		<requireLineOfSight>false</requireLineOfSight>
		<jobDef>VFEA_GotoTargetAndUseAbility</jobDef>
		<targetMode>Pawn</targetMode>
		<range>999999</range>
		<iconPath>Abilities/Puppeteer/BrainCut</iconPath>
		<abilityClass>Adjustments.Puppeteer_Adjustments.Ability_SoulGrowth</abilityClass>
		<castSound>VPEP_Brainleech_Cast</castSound>
		<distanceToTarget>1.5</distanceToTarget>
		<needsTicking>true</needsTicking>
		<drawAimPie>false</drawAimPie>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
			
		</targetingParameters>
		<castTime>50</castTime>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEP_Puppeteer</path>
				<level>4</level>
				<order>1</order>
				<psyfocusCost>0.25</psyfocusCost>
				<entropyGain>0</entropyGain>
				<psychic>True</psychic>
				<prerequisites>
					<li>VPEP_Puppet</li>
				</prerequisites>
			</li>
			<li Class="Adjustments.Puppeteer_Adjustments.ValidatePawnTarget">
			</li>
		</modExtensions>
		
	</VFECore.Abilities.AbilityDef>
	
	Ability_SoulMutation
	<VFECore.Abilities.AbilityDef >
	
		<defName>ADJ_SoulMutation</defName>
		
		<label>soul mutation</label>
		<description>Mutate the targets soul expanding upgrading his passions.</description>
		
		<warmupSound>VPE_PsycastCastingEffect</warmupSound>
        <warmupMote>VPE_Mote_CastPsycast</warmupMote>
		<gizmoClass>VanillaPsycastsExpanded.Command_Ability_Psycast</gizmoClass>
		<cooldownTime>20</cooldownTime>
		<rangeRingColor>(1, 1, 1)</rangeRingColor>
		<radiusRingColor>(0.3, 0.8, 1)</radiusRingColor>
		<requireLineOfSight>false</requireLineOfSight>
		<jobDef>VFEA_GotoTargetAndUseAbility</jobDef>
		<range>999999</range>
		<iconPath>Abilities/Puppeteer/BrainCut</iconPath>
		<abilityClass>Adjustments.Puppeteer_Adjustments.Ability_SoulMutation</abilityClass>
		<castSound>VPEP_Brainleech_Cast</castSound>
		<targetMode>Pawn</targetMode>
		<distanceToTarget>1.5</distanceToTarget>
		<needsTicking>true</needsTicking>
		<drawAimPie>false</drawAimPie>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<castTime>5</castTime>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPEP_Puppeteer</path>
				<level>4</level>
				<order>4</order>
				<psyfocusCost>.9</psyfocusCost>
				<entropyGain>0</entropyGain>
				<psychic>True</psychic>
				<prerequisites>
					<li>VPEP_Puppet</li>
				</prerequisites>
			</li>
			<li Class="Adjustments.Puppeteer_Adjustments.ValidatePawnTarget">
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>

	
